\n    opacity: 0;\n}\n\n.hamburger-menu_1606409355438_579 .hamburger-btn span:nth-child(3) {\n    transform-origin: -11px 4px;\n}\n.hamburger-menu_1606409355438_579.open .hamburger-btn span:nth-child(3) {\n    transform: rotate(-45deg) translate(0, 12px);\n}\n\n/* themes */\n\n\n</style>\n<!-- inspired by https://codepen.io/erikterwan/pen/EVzeRP -->\n<div class=\"hamburger-menu_1606409355438_579 closed\">\n    <div class=\"hamburger-btn fixed\">\n      <span></span>\n      <span></span>\n      <span></span>\n    </div>\n    <div class=\"nav-container\">\n      <div class=\"prevent-scale\">\n        <nav role=\"navigation\" class=\"\">\n            <ul>\n            \n                <li><a\n                    \n                        href=\"#!page-home\"\n                    \n                        linkType=\"LinkTypePage\"\n                    \n                        title=\"Home\"\n                    \n                >Home</a></li>\n            \n                <li><a\n                    \n                        href=\"#!page-resources\"\n                    \n                        linkType=\"LinkTypePage\"\n                    \n                >Resources</a></li>\n            \n                <li><a\n                    \n                        href=\"#!page-discussion_contact\"\n                    \n                        linkType=\"LinkTypePage\"\n                    \n                        title=\"Discussion_Contact\"\n                    \n                >Discussion_Contact</a></li>\n            \n            </ul>\n        </nav>\n      </div>\n  </div>\n</div>\n<script>\n(function() {\n  var isOpen = false;\n  function close() {\n    isOpen = false;\n    $('.hamburger-menu_1606409355438_579').removeClass('open');\n    $('.hamburger-menu_1606409355438_579').addClass('closed');\n  }\n  function open() {\n    isOpen = true;\n    $('.hamburger-menu_1606409355438_579').addClass('open');\n    $('.hamburger-menu_1606409355438_579').removeClass('closed');\n  }\n  function toggle() {\n    if(isOpen) close();\n    else open();\n  }\n  var silexElement = $('.hamburger-menu_1606409355438_579').closest('.editable-style');\n  function move(data) {\n    var offset = silexElement.offset();\n    $('.hamburger-menu_1606409355438_579 .nav-container').css({\n      'transform': 'translate(' + (data.scrollLeft - (offset.left/data.scale)) + 'px, ' + (data.scrollTop - (offset.top/data.scale)) + 'px)'\n    });\n  }\n  $('.hamburger-menu_1606409355438_579 .hamburger-btn').click(function() { toggle(); });\n  $(document).on('silex.scroll silex.resize', function(e) {\n    close();\n    move(window.silex);\n  });\n  close();\n})();\n</script>\n"}]}